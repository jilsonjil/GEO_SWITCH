        <activity
            android:name=".locationconnectivityview"
            android:exported="false" />
        <activity
            android:name=".locationmessageview"
            android:exported="false" />
        <activity
            android:name=".timemobileprofileview"
            android:exported="false" />
        <activity
            android:name=".locationmobileprofileview"
            android:exported="false" />
        <activity
            android:name=".locationreminderview"
            android:exported="false" />
        <activity
            android:name=".splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".userprofile"
            android:exported="false" />
        <activity
            android:name=".timereminder"
